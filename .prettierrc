{
  "arrowParens": "avoid",
  "trailingComma": "none",
  "singleQuote": true,
  "tabWidth": 2,
  "semi": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/store/(.*)$",
    "^@/services/(.*)$",
    "^@/components/(.*)$",
    "^@/screens/(.*)$",
    "^@/ui/(.*)$",
    "^@/shared/(.*)$",
    "^@/hooks/(.*)$",
    "^@/utils/(.*)$",
    "^@/constants/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}